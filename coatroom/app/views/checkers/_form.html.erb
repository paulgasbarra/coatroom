
<%= form_for @checker do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br/>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br/>
  <%= f.label :street_address %>
  <%= f.text_field :street_address %>
  <%= f.label :apt_number %>
  <%= f.text_field :apt_number %><br/>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %><br/>
  <%= f.label :phone_number %>
  <%= f.telephone_field :phone_number %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br/>
   <%= f.label :password %>
  <%= f.password_field :password %><br/>
   <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %><br/>
  <%= f.label :bank_account %>
  <%= f.text_field :bank_account %><br/>
  <%= f.label :bank_routing_number %>
  <%= f.text_field :bank_routing_number %><br/>
  <%= f.submit %>
<% end %>
